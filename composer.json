{
    "name": "glenville-webmaster/openedu",
    "description": "Template for Drupal 8 sites built using the OpenEDU distribution.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "drupal/coder": "~8.2.0",
        "phing/phing": "^2.14",
        "drupal/devel": "^1.0",
        "squizlabs/php_codesniffer": "~2.0",
        "drupal/console": "^1.7",
        "drush/drush": "^9.2"
    },
    "require": {
        "acquia/lightning": "~4.1.5",
        "drupal/address": "^1.8",
        "drupal/addtoany": "^1.14",
        "drupal/addtocal": "^1.0",
        "drupal/admin_toolbar": "^1.27",
        "drupal/better_exposed_filters": "^4.0",
        "drupal/better_normalizers": "~1.0",
        "drupal/calendar": "^1.0",
        "drupal/chosen": "^2.9",
        "drupal/date_recur": "^2.2",
        "drupal/default_content": "~1.0",
        "drupal/facets": "^1.4",
        "drupal/field_group": "^3.1",
        "drupal/flexible_layout": "^1.0",
        "drupal/flickity": "^2.2",
        "drupal/geocoder": "^3.0",
        "drupal/google_analytics": "^3.1",
        "drupal/google_analytics_reports": "~3.0",
        "drupal/google_map_field": "^1.12",
        "drupal/lingotek": "^3.0",
        "drupal/link_attributes": "^1.10",
        "drupal/name": "^1.0",
        "drupal/obfuscate_email": "^1.2",
        "drupal/sa11y": "~1.0",
        "drupal/search_api_autocomplete": "^1.3",
        "drupal/search_api_sorts": "^1.0",
        "drupal/simple_sitemap": "^3.7",
        "drupal/twig_field_value": "^2.0",
        "drupal/views_block_filter_block": "~1.0",
        "drupal/webform": "^5.17",
        "drupal/yoast_seo": "~2.0",
        "geocoder-php/google-maps-provider": "^4.5",
        "harvesthq/chosen": "^1.8"
    },
    "config": {
        "platform": {
            "php": "7.3"
        }
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "chosen": {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "./vendor/bin/phing githooks",
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "./vendor/bin/phing push"
        ],
        "post-update-cmd": [
            "./vendor/bin/phing push"
        ],
        "release-version": "ImageX\\OpenEDU\\Composer\\ReleaseVersion::execute"
    },
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/libraries/{$name}": [
                "type:bower-asset",
                "type:npm-asset",
                "type:drupal-library"
            ]
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/core": {
                "2924112 - Support class definition on field_layout": "https://www.drupal.org/files/issues/2924112-dynamic_layouts-7.patch"
            },
            "drupal/panels": {
                "Page title empty": "https://www.drupal.org/files/issues/2018-03-19/panels--page_title_does_not_display--2869412-27.patch",
                "2868828 - Support dynamic layout": "https://www.drupal.org/files/issues/2868828-dynamic-layout-plugin-definition-2.patch",
                "2921415 - Use region labels for IPE": "https://www.drupal.org/files/issues/use_labels-2921415-2.patch",
                "2930835 - Use Subformstate on settings form": "https://www.drupal.org/files/issues/2930835-subformstate-2.patch",
                "500 errors": "https://www.drupal.org/files/issues/panels-ipe-moderation-2919054-23.patch"
            },
            "drupal/panelizer": {
                "Preview new entities": "https://www.drupal.org/files/issues/2750491-4.patch",
                "Expose Panelizer through REST": "https://www.drupal.org/files/issues/panelizer-rest-2865575-5.patch"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ]
    },
    "autoload": {
        "psr-4": {
            "ImageX\\OpenEDU\\": "src"
        }
    }
}
